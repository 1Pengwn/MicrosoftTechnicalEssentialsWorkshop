<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- Which provider -->
    <add key="provider" value="System.Data.SqlClient" />

    <!-- Which connection string? Comment: Example; Using the app settings element to embed a connection string.
    <add key="connectionString" value="Data Source=(local)\SQLEXPRESS;Initial Catalog=AutoLot;Integrated Security=True"/>
    -->
  </appSettings>
  <!-- Comment: Using the connection string element, you can add multiple connection string name-value pairs -->
  <connectionStrings>
    <add name="DroneSqlProvider" connectionString="Data Source=(local)\SQLEXPRESS;Initial Catalog=AutoLot;Integrated Security=True"/>
  </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>